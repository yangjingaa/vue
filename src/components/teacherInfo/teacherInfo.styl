.personal-main
  display inline-block
  .top
    display block
    font-size 16px
    font-family "Monospaced Number", "Chinese Quote", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif
    text-align center
    width 400px
    margin-bottom 20px
  .writeInfo
    width 400px
    text-align center
    .input
      display flex
      justify-content flex-start
      margin-bottom 20px
      .font
        width 95px
        line-height 32px
    button
      margin-top 10px
      width 200px
      &:hover
        background azure

.table
  margin-top 20px

.topss
  display flex
  baseline-shift sub

.warning-content {
  height 300px
  overflow-y auto
  overflow-x hidden
}

.warning {
  padding 10px 15px
  border 1px solid #efefef
  margin-bottom 5px
}

.warning-top {
  font-size 13px
  border-bottom 1px solid #efefef
}

.warning-con {
  font-size 15px
  padding-top: 10px
}

.back {
  margin-bottom 10px
  margin-left 20px
}

